function setup() {
  createCanvas(800, 800);
  background(0)
  fill(255)
  textAlign(CENTER);
  textSize(32);
  text("dibujando", width/2, 100);
  
}

function draw() {
  //background (255, 0, 255)
  fill(random(100, 200), 0, random(100, 200), 30);
  ellipse(mouseX, mouseY, 100, 100);
  
 
}

function mousePressed (){
  console.log ("clic");
   background(0);
  fill(255)
  textAlign(CENTER); 
  textSize(32);
  text("cositas", width/2, 100); 
}

function keyPressed() {
  console.log ("key");
  backgrond (0); 
